				{
					"name": "validate-otp",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// 1. Basic check to ensure the request was successful",
									"pm.test(\"Status code is 200 OK\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"// 2. Parse the JSON response body into a JavaScript object",
									"try {",
									"    const responseData = pm.response.json();",
									"    ",
									"    // 3. Validate that the required field exists",
									"    pm.expect(responseData).to.be.an('object');",
									"    pm.expect(responseData).to.have.property('accessToken');",
									"    ",
									"    const token = responseData.accessToken;",
									"",
									"    // 4. Check if the token is a non-empty string",
									"    if (token && typeof token === 'string' && token.length > 0) {",
									"        ",
									"        // Set the value as a Postman Environment Variable",
									"        // Make sure you have an active environment selected in Postman!",
									"        pm.environment.set(\"access_token\", token);",
									"        ",
									"        console.log(\"Successfully retrieved and set Access Token.\");",
									"        console.log(\"Stored token value: \" + token.substring(0, 15) + \"...\"); // Print partial token for security",
									"    } else {",
									"        pm.expect.fail(\"accessToken found in response but was null or empty.\");",
									"    }",
									"    ",
									"} catch (e) {",
									"    // Handle cases where parsing failed (e.g., non-JSON response)",
									"    pm.expect.fail(\"Failed to parse JSON response body or unexpected format.\");",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
